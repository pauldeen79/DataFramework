    <PackageId>pauldeen79.DataFramework.ModelFramework</PackageId>
    <RepositoryUrl>https://github.com/pauldeen79/DataFramework</RepositoryUrl>
    <CodeAnalysisRuleSet>..\..\.sonarlint\pauldeen79_dataframeworkcsharp.ruleset</CodeAnalysisRuleSet>
